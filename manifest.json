{
  "manifest_version": 3,
  "name": "UMPSA Exam Enhancer",
  "version": "0.1.0",
  "description": "Improves the interface and functionality of the UMPSA exam paper website, making it easier for students to browse, search, and access past exam papers.",
  "author": "Naz",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://apps-oss-ump-edu-my.libraryumpsa.idm.oclc.org/exam/searchProcess.php"],
  "content_scripts": [
    {
      "matches": ["https://apps-oss-ump-edu-my.libraryumpsa.idm.oclc.org/exam/searchProcess.php"],
      "js": ["content-script.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["bundle.js"],
      "matches": ["https://*.oclc.org/*"]
    }
  ]
}
